{
  "name": "loadAPI",
  "api_id": "loadAPI",
  "use_keyless": true,
  "active": true,
  "proxy": {
    "listen_path": "/loadAPI/",
    "target_url": "http://host.docker.internal:5000", //http://host.docker.internal:5000/api/v1
    "strip_listen_path": true,

    //load balancing
    "enable_load_balancing": false,
    "target_list": [
    ]

  },
  "version_data": {
    "not_versioned": true,
    "default_version": "string",
    "versions": {
      "default": {
        "name": "string",
        "use_extended_paths": true
      }
    }
  },

  //rate limit
  "global_rate_limit": {
    "rate": 0,
    "per": 0
  }
}
